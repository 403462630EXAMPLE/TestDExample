        <!--style="@style/mProgress_horizontal"-->
        <!--android:id="@+id/pb_loading"-->
        <!--android:progress="0"-->
        <!--android:max="100"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"/>-->
</LinearLayout>
